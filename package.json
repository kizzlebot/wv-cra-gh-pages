{
  "name": "webviewer-sb",
  "version": "0.1.0",
  "private": true,
  "port": 3030,
  "dependencies": {
    "@enotarylog/ramda": "^1.0.0",
    "@pdftron/webviewer": "^6.2.3",
    "axios": "^0.19.2",
    "bluebird": "^3.7.2",
    "bootstrap": "^4.5.0",
    "debug": "^4.1.1",
    "faker": "^4.1.0",
    "firebase": "^7.15.0",
    "isomorphic-unfetch": "^3.0.0",
    "node-sass": "^4.14.1",
    "polished": "^3.5.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-use": "^14.2.0",
    "styled-components": "^5.1.1",
    "twilio-video": "^2.3.0"
  },
  "scripts": {
    "download-webviewer": "npx @pdftron/webviewer-downloader --path ./public/lib --v 6.3.2 --npm",
    "start": "PORT=3030 react-scripts start",
    "dev": "PORT=3030 react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@storybook/addon-a11y": "^5.3.18",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-backgrounds": "^5.3.18",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-create-react-app": "^2.1.1",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^9.3.2",
    "@testing-library/react-hooks": "^3.3.0",
    "@testing-library/user-event": "^7.1.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-canvas-mock": "^2.2.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.4",
    "react-hot-loader": "^4.12.21",
    "react-test-renderer": "^16.13.1",
    "sass-loader": "^8.0.2",
    "storybook-dark-mode": "^0.4.1",
    "webpack": "4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "!src/**/*.d.ts",
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/setupTestEnv",
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "react-app"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  }
}
